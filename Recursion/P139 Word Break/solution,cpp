#include <bits/stdc++.h>
using namespace std;


class Solution {
public:
    bool wordBreak(string s, vector<string>& wordDict) {
        vector<bool> dp(s.size() + 1, false);
        dp[0] = true;
        
        for (int i = 0; i < s.size() + 1; i++) {
            if (dp[i]) {
                for (const string& word : wordDict) {
                    if (i + word.length() <= s.size() && 
                        s.substr(i, word.length()) == word) { // Check if the substring matches the word
                        dp[i + word.length()] = true; // Mark the position after the word as reachable
                    }
                }
            }
        }
        
        return dp[s.size()]; // Return whether the entire string can be segmented
    }
};

int main() {
    Solution sol;
    string s = "leetcode";
    vector<string> wordDict = {"leet", "code"};
    cout << (sol.wordBreak(s, wordDict) ? "true" : "false") << endl;
    return 0;
}